<idea-plugin>
    <id>com.andromeda.routes-plugin</id>
    <name>Route Inlay Hints</name>
    <vendor email="andromeda.coders@gmail.com" url="https://true.fishing">Andromeda Coders</vendor>
    <description>Route inlay hints for controller actions</description>
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>
    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.inlayProvider language="JAVA" implementationClass="com.andromeda.routesplugin.RouteInlayHintsProvider"/>
    </extensions>
</idea-plugin>
